    "psi-header.config": {
        "author": "Chong Liu"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "modAuthor": "Modified By:",
        "modDate": "Last Modified:",
        "autoHeader": "autoSave"
    },
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "@File: <<filename>>",
                "@Created Date: <<filecreated('YYYY-MM-DD h:mm:ss a')>>",
                "@Author: <<author>>",
                "@Brief: Description of what this file does",
                "=================================================================================",
                "Copyright (c) <<yeartoyear(fc, now)>> <<author>>",
                "=================================================================================",
                "Last Modified: <<author>> - <<dateformat('YYYY-MM-DD h:mm:ss a')>>"
            ]
        }
    ],
    "git.openRepositoryInParentFolders": "never",
    "KeilAssistant.MDK.Uv4Path": "D:\\Developments\\Keil\\UV4\\UV4.exe",
    // 头部注释
    "fileheader.customMade": {
        "FilePath": "Do not edit", // 增加此项配置即可
        "Author": "git config user.name", // 同时获取用户名与邮箱
        "Date": "Do not edit", // 文件创建时间(不变)
        "LastEditors": "git config user.name", // 文件最后编辑者 与Author字段一致
        "LastEditTime": "Do not edit", // 文件最后编辑时间
        "custom_string_obkoro1": "=================================================================================",
        "custom_string_obkoro1_copyright": "Copyright (c) ${now_year} by git config user.name, All Rights Reserved. ",
        "custom_string_obkoro2": "=================================================================================",
        "Description": "" // 介绍文件的作用、文件的入参、出参。
    },
    // 函数注释
    "fileheader.cursorMode": {
        "description": "", // 函数注释生成之后，光标移动到这里
        "custom_string_obkoro1": "=================================================================================",
        "param": "", // param 开启函数参数自动提取 需要将光标放在函数行或者函数上方的空白行
        "return": ""
    },
    // 插件配置项
    "fileheader.configObj": {
        "autoAdd": false, // 检测文件没有头部注释，自动添加文件头部注释
        "wideSame": true, // 设置为true开启
        "wideNum": 13, // 字段长度 默认为13
        // 自动添加头部注释黑名单
        "prohibitAutoAdd": [
            "json",
            "md"
        ],
        "moveCursor": true, // 自动移动光标到Description所在行
        "throttleTime": 60000, // 对同一个文件 需要过60秒钟再次修改文件并保存才会更新注释
        "specialOptions":{
            "Date": "CreateDate"
        }
    }
}