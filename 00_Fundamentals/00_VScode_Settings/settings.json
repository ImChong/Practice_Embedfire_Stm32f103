    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "#000000",
        "background": "#ffffff",
        "opacity": 50,
        "iconColour": "gray",
        "gutterIcon": true
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow"
        },
        "NOTE": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray"
        },
        "COMMENT": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray"
        },
        "FIXME": {
            "foreground": "black",
            "background": "orange",
            "iconColour": "orange"
        },
        "BUG": {
            "icon": "bug",
            "foreground": "black",
            "background": "red",
            "iconColour": "red"
        },
        "[ ]": {
            "icon": "check",
            "foreground": "black",
            "background": "white",
            "iconColour": "white"
        },
        "[x]": {
            "icon": "check",
            "foreground": "white",
            "background": "green",
            "iconColour": "green"
        },
        "DONE": {
            "icon": "check",
            "foreground": "white",
            "background": "green",
            "iconColour": "green"
        },
        "#if": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "white",
            "background": "blue",
            "iconColour": "blue"
            // "gutterIcon": false,
        },
        "#endif": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "white",
            "background": "blue",
            "iconColour": "blue"
            // "gutterIcon": false,
        },
        "#elif": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "white",
            "background": "#1085c9",
            "iconColour": "#1085c9"
            // "gutterIcon": false,
        },
        "#else": {
            "icon": "diff-renamed",
            "type": "line",
            "foreground": "white",
            "background": "#1085c9",
            "iconColour": "#1085c9"
            // "gutterIcon": false,
        },
        "Functionality": {
            "icon": "note",
            "type": "line",
            "foreground": "white",
            "background": "green",
            "iconColour": "green"
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "NOTE",
        "COMMENT",
        "[ ]",
        "[x]",
        "DONE",
        "#if",
        "#endif",
        "#elif",
        "#else",
        "Functionality"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
